{
  "name": "env-manager-cli",
  "description": "a manager for local and heroku environmental variables",
  "version": "1.0.2",
  "main": "./lib/index.js",
  "dependencies": {
    "colors": "^1.2.1",
    "inquirer": "^5.2.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "./node_modules/.bin/eslint lib/**",
    "prepublish": "yarn lint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brantron/dotenv-manager.git"
  },
  "keywords": [
    "heroku",
    "env",
    "dotenv",
    "cli"
  ],
  "author": "Brandon Lawrence",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Brantron/dotenv-manager/issues"
  },
  "homepage": "https://github.com/Brantron/dotenv-manager#readme",
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/lib/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-plugin-prettier": "^2.6.0",
    "inquirer-test": "^2.0.1",
    "jest": "^22.4.3",
    "memfs": "^2.8.0",
    "prettier": "^1.12.0"
  }
}
